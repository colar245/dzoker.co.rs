          <span>011 318 56 69<br><br> 069 318 56 69</span>
        </div>
      </div>
      <div class="email kontakt">
        <i class="fa-solid fa-envelope"></i>
        <div class="txt">
          <h1>Email</h1>
          <span>{{ data.email }}</span>
        </div>
      </div>
    </div>
